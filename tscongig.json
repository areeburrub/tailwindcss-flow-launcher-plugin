{
  "compilerOptions": {
    "target": "es2016",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strict": true,
    "skipLibCheck": true,
    "removeComments": true,
    "outDir": "./build"
  }
}
